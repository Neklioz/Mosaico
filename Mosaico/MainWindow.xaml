<Window x:Class="Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Height="600" Width="600" MinWidth="450" MinHeight="470"
        Icon="./assets/mime_theme_colors.ico">
    <DockPanel>
        <TextBlock Background="BlueViolet" DockPanel.Dock="Left" Width="100"/>
        <TextBlock Background="BlueViolet" DockPanel.Dock="Top" Height="100"/>
        <TextBlock Background="Green" DockPanel.Dock="Right" Width="100" />
        <TextBlock Background="Green" DockPanel.Dock="Bottom" Height="100" />
        <TextBlock Background="OrangeRed" DockPanel.Dock="Left" Width="100" />
        <TextBlock Background="OrangeRed" DockPanel.Dock="Bottom" Height="100" />
        <TextBlock Background="DeepPink" DockPanel.Dock="Right" Width="100" />
        <TextBlock Background="DeepPink" DockPanel.Dock="Top" Height="100" />
        <TextBlock Background="Black"/>
    </DockPanel>
</Window>
